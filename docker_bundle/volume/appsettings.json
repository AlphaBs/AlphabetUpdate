{
  "Urls": "http://0.0.0.0:5000",
  "PathBase": "/updateserver",
  
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "UseSecureAesStorage": false,
  "SecureStorageKey": "",
  "SecureStorageIV": "",
  "Auth": {
    "Scheme": "basic",
    "Issuer": "AlphabetDevTestServer",
    "ExpiresM": 60
  },
  "Users": [
    {
      "Username": "admin",
      "Password": "admin",
      "Role": "manager"
    }
  ],
  "UpdateFile": {
    "Name": "AlphabetDevTestServer",
    "InputDir": "/volume/in",
    "OutputDir": "/volume/out",
    "BaseUrl": "http://localhost/files",
    "LauncherInfoPath": "/volume/launcherInfo.json",
    "FilesCachePath": "/volume/files.json"
  }
}